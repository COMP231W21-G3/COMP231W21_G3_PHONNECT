<VOICEMAPPING>
  <!-- FUNCTION specified by an action key and object key-->
  <FUNCTION>
    <FUNCTION_NAME>nav_chatrooms</FUNCTION_NAME>
    <ACTION_KEY>nav</ACTION_KEY>
    <OBJECT_KEY>chatroom</OBJECT_KEY>
  </FUNCTION>  
  <FUNCTION>
    <FUNCTION_NAME>nav_feed</FUNCTION_NAME>
    <ACTION_KEY>nav</ACTION_KEY>
    <OBJECT_KEY>feed</OBJECT_KEY>
  </FUNCTION>
  <FUNCTION>
    <FUNCTION_NAME>nav_profile</FUNCTION_NAME>
    <ACTION_KEY>nav</ACTION_KEY>
    <OBJECT_KEY>profile</OBJECT_KEY>
  </FUNCTION>
  <FUNCTION>
    <FUNCTION_NAME>do_createpost</FUNCTION_NAME>
    <ACTION_KEY>do</ACTION_KEY>
    <OBJECT_KEY>post</OBJECT_KEY>
  </FUNCTION>
    <FUNCTION>
    <FUNCTION_NAME>meet_stranger</FUNCTION_NAME>
    <ACTION_KEY>meet</ACTION_KEY>
    <OBJECT_KEY>stranger</OBJECT_KEY>
  </FUNCTION>
  <FUNCTION>
    <FUNCTION_NAME>search_user</FUNCTION_NAME>
    <ACTION_KEY>search</ACTION_KEY>
    <OBJECT_KEY>user</OBJECT_KEY>
  </FUNCTION>
  <FUNCTION>
    <FUNCTION_NAME>sign_out</FUNCTION_NAME>
    <ACTION_KEY>sign</ACTION_KEY>
    <OBJECT_KEY>out</OBJECT_KEY>
  </FUNCTION>
  <!-- <FUNCTION>
    <FUNCTION_NAME>do_message</FUNCTION_NAME>
    <ACTION_KEY>do</ACTION_KEY>
    <OBJECT_KEY>message</OBJECT_KEY>
  </FUNCTION> -->


  <!-- ACTION - Verb - To do something-->
  <ACTION>
    <ACTION_ACTION_KEY>nav</ACTION_ACTION_KEY>
    <ACTION_KEYWORDS>go~navigate~move~take~show~find~bring</ACTION_KEYWORDS>
    <ACTION_SUGGESTIONS>Take me to my chatrooms~Go to my profile~Show my feed</ACTION_SUGGESTIONS>
  </ACTION>
  <ACTION>
    <ACTION_ACTION_KEY>do</ACTION_ACTION_KEY>
    <ACTION_KEYWORDS>send~create~start~do~generate</ACTION_KEYWORDS>
    <ACTION_SUGGESTIONS>Create a post</ACTION_SUGGESTIONS>
  </ACTION>
  <ACTION>
    <ACTION_ACTION_KEY>meet</ACTION_ACTION_KEY>
    <ACTION_KEYWORDS>meet</ACTION_KEYWORDS>
    <ACTION_SUGGESTIONS>Meet A Stranger</ACTION_SUGGESTIONS>
  </ACTION>
  <ACTION>
    <ACTION_ACTION_KEY>search</ACTION_ACTION_KEY>
    <ACTION_KEYWORDS>search~find</ACTION_KEYWORDS>
    <ACTION_SUGGESTIONS>Search User</ACTION_SUGGESTIONS>
  </ACTION>
  <ACTION>
    <ACTION_ACTION_KEY>sign</ACTION_ACTION_KEY>
    <ACTION_KEYWORDS>sign~log</ACTION_KEYWORDS>
    <ACTION_SUGGESTIONS>Sign Out</ACTION_SUGGESTIONS>
  </ACTION>

  <!-- OBJECTS - Noun - Destination - Physical -->
  <!-- <OBJECT>
    <OBJECT_OBJECT_KEY>message</OBJECT_OBJECT_KEY>
    <OBJECT_KEYWORDS>message</OBJECT_KEYWORDS>
    <OBJECT_SUGGESTIONS>Send a message</OBJECT_SUGGESTIONS>
  </OBJECT> -->
  <OBJECT>
    <OBJECT_OBJECT_KEY>chatroom</OBJECT_OBJECT_KEY>
    <OBJECT_KEYWORDS>chatroom~chatrooms~groupchat~chats~chat~groupchats</OBJECT_KEYWORDS>
    <OBJECT_SUGGESTIONS>Go to my chatrooms</OBJECT_SUGGESTIONS>
  </OBJECT>
  <OBJECT>
    <OBJECT_OBJECT_KEY>feed</OBJECT_OBJECT_KEY>
    <OBJECT_KEYWORDS>feed~feeds~feet</OBJECT_KEYWORDS>
    <OBJECT_SUGGESTIONS>Go to my feed</OBJECT_SUGGESTIONS>
  </OBJECT>
  <OBJECT>
    <OBJECT_OBJECT_KEY>profile</OBJECT_OBJECT_KEY>
    <OBJECT_KEYWORDS>profile</OBJECT_KEYWORDS>
    <OBJECT_SUGGESTIONS>Go to my profile</OBJECT_SUGGESTIONS>
  </OBJECT>
  <OBJECT>
    <OBJECT_OBJECT_KEY>post</OBJECT_OBJECT_KEY>
    <OBJECT_KEYWORDS>post</OBJECT_KEYWORDS>
    <OBJECT_SUGGESTIONS>Create a content post</OBJECT_SUGGESTIONS>
  </OBJECT>
  <OBJECT>
    <OBJECT_OBJECT_KEY>stranger</OBJECT_OBJECT_KEY>
    <OBJECT_KEYWORDS>stranger~strangers</OBJECT_KEYWORDS>
    <OBJECT_SUGGESTIONS>Meet a Stranger</OBJECT_SUGGESTIONS>
  </OBJECT>
  <OBJECT>
    <OBJECT_OBJECT_KEY>user</OBJECT_OBJECT_KEY>
    <OBJECT_KEYWORDS>user</OBJECT_KEYWORDS>
    <OBJECT_SUGGESTIONS>Search User</OBJECT_SUGGESTIONS>
  </OBJECT>
  <OBJECT>
    <OBJECT_OBJECT_KEY>out</OBJECT_OBJECT_KEY>
    <OBJECT_KEYWORDS>out</OBJECT_KEYWORDS>
    <OBJECT_SUGGESTIONS>Sign out</OBJECT_SUGGESTIONS>
  </OBJECT>

</VOICEMAPPING>